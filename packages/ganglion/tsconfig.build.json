{
  "compilerOptions": {
    "outDir": "build",

    "emitDeclarationOnly": false,
    "declaration": true,

    "target": "esnext",
    "newLine": "LF",
    "moduleResolution": "node",
    "strict": true,
    "jsx": "preserve",
    "jsxImportSource": "solid-js"
  },

  "exclude": ["node_modules"]
}